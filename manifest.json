{
   "author": "Anatol Liebermann",
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": { 
      "default_icon": "images/popup.png", 
      "default_popup": "popup.html"
   },
   "content_security_policy": "style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_Desc__",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   "manifest_version": 2,
   "name": "__MSG_Name__",
   "options_page": "options.html",
   "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
   "permissions": ["storage", "alarms", "notifications", "contextMenus", "tabs", "http://commondatastorage.googleapis.com/*", "http://sourceforge.net/projects/osxportableapps/*", "https://omahaproxy.appspot.com/*", "cookies" ],
   "short_name": "Chromium Updater",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.1.0.3"
}
